{
    "name": "alcohol/sf-minimal-demo",
    "description": "A minimalistic Symfony demo application",
    "keywords": ["symfony", "demo", "minimal", "skeleton", "sf"],
    "homepage": "https://github.com/alcohol/sf-minimal-demo",
    "license": "MIT",
    "authors": [{
        "name": "Rob Bast",
        "email": "rob.bast@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/alcohol/sf-minimal-demo/issues",
        "source": "https://github.com/alcohol/sf-minimal-demo"
    },
    "require": {
        "php": "~5.6",
        "symfony/monolog-bundle": "~2.6",
        "predis/predis": "~1.0",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "guzzle/guzzle": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Alcohol\\": ["app/"],
            "Alcohol\\PasteBundle\\": ["src/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
